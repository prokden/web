    "bootstrap-icons": "^1.10.3",
    "esbuild": "^0.17.4",
    "jquery": "^3.6.4",
    "sass": "^1.57.1",
    "select2": "^4.1.0-rc.0",
    "select2-bootstrap-5-theme": "^1.3.0",
